{
  "name": "SanAndreas-Problem",
  "version": "0.0.1",
  "description": "SanAndreas Problem - Take Senior developer test",
  "main": "main.js",
  "repository": "https://github.com/LucasCFM/SanAndreas-Take",
  "author": "Lucas Mendes (github.com/LucasCFM)",
  "private": true,
  "type": "module",
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "start": "node ./src/main.js"
  },
  "_moduleAliases": {
    "@": "./src",
    "@utils": "./src/utils"
  },
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "html"
    ],
    "moduleNameMapper": {
      "@/(.*)": "<rootDir>/src/$1",
      "@utils/(.*)": "<rootDir>/src/utils/$1"
    }
  },
  "devDependencies": {
    "jest": "^26.4.2"
  },
  "dependencies": {
    "lodash": "^4.17.20",
    "module-alias": "^2.2.2"
  }
}
